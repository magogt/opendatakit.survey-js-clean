{
    "model": {
        "bird": {
            "type": "string"
        }
    }, 
    "settings": [
        {
            "setting": "formId", 
            "value": "selectImage"
        }, 
        {
            "setting": "formVersion", 
            "value": 1
        }, 
        {
            "setting": "formTitle", 
            "value": {
                "default": "selectImage", 
                "english": "Select image test."
            }
        }
    ], 
    "survey": [
        {
            "prompts": [
                {
                    "type": "select_one", 
                    "name": "bird", 
                    "param": "birds", 
                    "label": "Choose a bird:"
                }, 
                {
                    "type": "note", 
                    "param": null, 
                    "condition": "selected(data('bird'), 'blackbird')", 
                    "label": "Blackbird selected"
                }, 
                {
                    "type": "note", 
                    "param": null, 
                    "condition": "selected(data('bird'), 'blackbird') && now().getHours() < 2", 
                    "label": "in the dead of night"
                }
            ], 
            "type": "screen"
        }
    ], 
    "calculates": [], 
    "choices": {
        "birds": [
            {
                "image": "media/blackbird.png", 
                "name": "blackbird"
            }, 
            {
