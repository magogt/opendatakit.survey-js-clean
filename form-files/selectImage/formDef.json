                "name": "bluethroat"
            }, 
            {
                "image": "media/crow.png", 
                "name": "crow"
            }, 
            {
                "image": "media/eagle.png", 
                "name": "eagle"
            }, 
            {
                "image": "media/egret.png", 
                "name": "egret"
            }, 
            {
                "image": "media/goose.png", 
                "name": "goose"
            }, 
            {
                "image": "media/gull.png", 
                "name": "gull"
            }, 
            {
