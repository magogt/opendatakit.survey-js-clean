        {
            "param": null, 
            "name": "axillary_temperature", 
            "required": true, 
            "type": "number", 
            "condition": "selected({{axillary_temperature_possible}}, 'yes')", 
            "label": "Measure axillary temperature:"
        }, 
        {
            "prompts": [
                {
                    "type": "select_one", 
                    "name": "fever_present", 
                    "param": "present", 
                    "label": "Feel  for fever:"
