            "param": "yes_no", 
            "type": "select_one"
        }, 
        {
            "prompts": [
                {
                    "type": "note", 
                    "param": null, 
                    "label": "How old is the child?"
                }, 
                {
                    "param": null, 
                    "name": "age_years", 
                    "comments": "required might cause problems here", 
                    "type": "integer", 
                    "condition": "selected({{age_lt_2mo}}, 'no')", 
                    "label": "Years"
                }, 
                {
                    "type": "integer", 
                    "name": "age_months", 
                    "param": null, 
                    "label": "Months"
                }, 
                {
                    "name": "age_weeks", 
                    "param": null, 
                    "type": "integer", 
                    "condition": "selected({{age_lt_2mo}}, 'yes')", 
                    "label": "Weeks"
                }
