                    "name": "refer_urgently_to_hospital_w_ORS", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Refer URGENTLY to hospital \nwith mother giving frequent sips \nof ORS on the way."
                }, 
                {
                    "name": "advise_continue_breastfeeding", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Advise mother to continue \nbreastfeeding."
                }, 
                {
                    "name": "advise_keep_infant_warm", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Advise to keep the young infant warm."
                }
