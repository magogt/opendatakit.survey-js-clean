            "type": "goto", 
            "param": "diarrhoea_section"
        }, 
        {
            "type": "label", 
            "param": "psbi"
        }, 
        {
            "prompts": [
                {
                    "name": "possible_serious_bacterial_infection_agree", 
                    "hint": "SIGNS:\n<ul>\n<li>Convulsions or</li>\n<li>Fast breathing (60 breaths per minute or more) or</li>\n<li>Severe chest indrawing or</li>\n<li>Nasal flaring or</li>\n<li>Grunting or</li>\n<li>10 or more skin pustules or a big boil or</li>\n<li>If axillary temperature 37.5°C or above (or feels hot to touch) or temperature less than 35.5°C (or feels cold to touch) or</li>\n<li>Lethargic or unconscious or</li>\n<li>Less than normal movements</li>\n</ul>", 
                    "type": "note", 
                    "param": null, 
                    "label": "CLASSIFICATION: <strong>POSSIBLE SERIOUS BACTERIAL INFECTION</strong>"
                }, 
                {
                    "type": "note", 
                    "param": null, 
                    "label": "<strong>This child needs URGENT attention, complete the assessment and pre-referral treatment immediately so referral is not delayed.</strong>"
                }, 
                {
                    "param": null, 
                    "name": "give_first_dose_of_cotrimoxazole", 
                    "required": true, 
                    "type": "acknowledge", 
                    "comments": "there are some charts for this and for consoling the mother, not sure if they should be used here.", 
                    "label": "Give first dose of cotrimoxazole if able to take orally (½ Pediatric tablet for an infant up to 1 month and 1 tablet for an infant 1-2 months) "
                }, 
                {
                    "name": "advise_continue_breastfeeding", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Advise mother to continue breastfeeding"
                }, 
                {
                    "name": "warm_infant_skin_to_skin", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Warm the young infant by Skin to Skin contact if feels cold to touch."
                }, 
                {
                    "name": "refer_urgently_to_hospital", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Refer URGENTLY to hospital."
                }
            ], 
            "type": "screen", 
            "name": "possible_serious_bacterial_infection"
        }, 
        {
            "type": "goto", 
            "param": "diarrhoea_section"
        }, 
        {
            "required": true, 
            "type": "label", 
            "param": "lbi"
        }, 
        {
            "prompts": [
                {
                    "name": "local_bacterial_infection_agree", 
                    "hint": "SIGNS:\n<ul>\n<li>Umbilicus red or draining pus</li>\n<li>Skin pustules less than 10</li>\n</ul>", 
                    "type": "note", 
                    "param": null, 
                    "label": "CLASSIFICATION: LOCAL BACTERIAL INFECTION"
                }, 
                {
                    "param": null, 
                    "name": "give_oral_cotrimoxazole_5_days", 
                    "required": true, 
                    "type": "acknowledge", 
                    "comments": "there are some charts for this and for consoling the mother, not sure if they should be used here.", 
                    "label": "Give oral cotrimoxazole for 5 days. (½ Pediatric tablet twice daily for an infant upto 1 month and 1 tablet for an infant 1-2 months)"
                }, 
                {
                    "name": "teach_mother_to_apply_gv_paint", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Teach mother to apply 0.5% Gentian Violet paint twice daily."
                }, 
                {
                    "name": "follow_up_2_days", 
                    "required": true, 
                    "type": "acknowledge", 
                    "param": null, 
                    "label": "Follow-up in 2 days."
                }
            ], 
            "type": "screen", 
            "name": "local_bacterial_infection"
        }, 
        {
            "required": true, 
            "type": "label", 
            "param": "diarrhoea_section"
        }, 
