        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{hot_to_touch}}, 'yes')"
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{lethargic}}, 'yes')"
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{abnormal_movements}},'yes')"
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{gt_10_skin_pustules}}, 'yes')"
        }, 
