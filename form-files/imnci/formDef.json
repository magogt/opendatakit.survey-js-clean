            "type": "screen", 
            "condition": "selected({{axillary_temperature_possible}}, 'no')"
        }, 
        {
            "name": "lethargic", 
            "required": true, 
            "type": "select_one", 
            "param": "yes_no", 
            "label": "Is the young infant is lethargic or unconscious?"
        }, 
        {
            "name": "abnormal_movements", 
            "required": true, 
            "type": "select_one", 
            "param": "yes_no", 
            "label": "Look at the young infant’s movements. Are they less than normal? "
        }, 
        {
            "name": "gt_10_skin_pustules", 
            "required": true, 
            "type": "select_one", 
            "param": "yes_no", 
            "label": "Look for skin pustules. Are there 10 or more skin pustules or a big boil?"
        }, 
        {
            "param": "psbi", 
            "type": "goto_if", 
            "condition": "{{breath_count}} >= 60", 
            "comments": "Idea: Make conditional gotos display alerts that indicate which condition they were triggered by. This way we can explain to users why certain paths are chosen which will help ensure that it is the correct one."
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{chest_indrawing}}, 'yes')"
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{nasal_flaring}}, 'yes')"
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{grunting}}, 'yes')"
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "{{axillary_temperature}} >= 37.5 || {{axillary_temperature}} < 35.5"
