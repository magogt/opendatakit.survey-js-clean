            "type": "screen"
        }, 
        {
            "type": "number", 
            "name": "weight", 
            "param": null, 
            "label": "Weight in kilograms:"
        }, 
        {
            "name": "initial_visit", 
            "param": null, 
            "type": "datetime", 
            "comments": "Are these date questions meant to be dates or boolean indicators of what type of visit the current visit is?", 
            "label": "Initial Visit:"
        }, 
        {
            "type": "datetime", 
            "name": "follow_up_visit", 
            "param": null, 
            "label": "Follow-up Visit:"
        }, 
        {
            "name": "convulsions", 
            "required": true, 
            "type": "select_one", 
            "param": "yes_no", 
            "label": "ASK: Has the child had convulsions?"
        }, 
        {
            "type": "goto_if", 
            "param": "psbi", 
            "condition": "selected({{convulsions}}, 'yes')"
        }, 
        {
            "name": "breath_count", 
            "required": true, 
            "type": "integer", 
            "label": "Count the breaths in one minute. Repeat the count if elevated.", 
            "param": null, 
            "templatePath": "breathCounter.handlebars"
        }, 
        {
            "name": "chest_indrawing", 
            "hint": "YOUNG INFANT MUST BE  CALM", 
            "required": true, 
            "type": "select_one", 
            "param": "present", 
            "label": "Look for severe chest  indrawing."
        }, 
        {
            "name": "nasal_flaring", 
            "hint": "YOUNG INFANT MUST BE  CALM", 
            "required": true, 
            "type": "select_one", 
            "param": "present", 
            "label": "Look for nasal flaring."
        }, 
        {
            "name": "grunting", 
            "hint": "YOUNG INFANT MUST BE  CALM", 
            "required": true, 
            "type": "select_one", 
            "param": "present", 
            "label": "Look and listen for grunting."
        }, 
        {
            "name": "axillary_temperature_possible", 
            "required": true, 
            "type": "select_one", 
            "param": "yes_no", 
